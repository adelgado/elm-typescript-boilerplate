.oscillators
    display flex
    justify-content center
    width 100%

.oscillators__osc1
    width 25%
    border-right solid 2px $section-title-background-color
    

.oscillators__osc2
    display flex

    flex-direction row
    justify-content space-between

.fix-me
    display flex
    flex-direction column
    width 75%
    justify-content space-around

.fix-me2
    margin-left 140px

.oscillators__extra
    display flex
    justify-content space-around
    border-top solid 2px $section-title-background-color


.oscillators__label
    font-weight bold
    text-align center
    color $section-title-background-color
    font-size 140%
    user-select none
    bottom 0

