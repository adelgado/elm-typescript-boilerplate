$rack-border-shadow = #450202

.dashboard
	height 100vh
	width 100vw
	display flex
	flex-direction column
	box-shadow inset 3px 3px 10px 1px $rack-border-shadow
	justify-content space-between
	align-items center

.dashboard__brand
	margin $unit
	color $text-color
	margin-left auto
	&::before
		content "web lead"